{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "settings": {
        "import/resolver": {
          "node": {
            "extensions": [".js", ".jsx", ".ts", ".tsx"]
          }
        }
      },
    "rules": {
        "no-console": 0,
        "consistent-return": 0,
        "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
        "object-curly-newline": ["error", { "multiline": true }],
        "import/extensions": [
            "error",
            "ignorePackages",
            { "js": "never", "jsx": "never", "ts": "never", "tsx": "never" }
         ]
    }
}
